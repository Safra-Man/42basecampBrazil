#include <unistd.h>

void ft_print_comb(void)
{
	// 789 , 012, 014, etc..
	// ijk => i < j e sei que j < k
	// Do entendimento do enunciado
	// sei que o i vai estar entre 0 e 7
	// sei que o j vai estar entre 1 e 8
	// sei que o k vai estar entre 2 e 9

	char i;
	char j;
	char k;

	i = '0';
	j = '1';
	k = '2';

	while (i <= '7')
	{
		while (j <= '8')
		{
			while (k <= '9')
			{
				if (i < j && j < k)
				{
					write(1, &i, 1);
					write(1, &j, 1);
					write(1, &k, 1);
					if (i != '7')
					{
						write(1, ", ", 1);
					}
				}
				k++;	
			}
			j++;
			k = '2';
		}
		i++;
		j = '1';
	}
}