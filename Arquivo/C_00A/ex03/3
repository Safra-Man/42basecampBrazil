#include <unistd.h>
// 0 = 48 e 9 = 57;

void	ft_print_numbers(void)
{
	char c;

	c = '0';
	while (c <= '9')
	{
		write(1, &c, 1);
		c++;
	}
	
	/*
	//Segunda forma de fazer:
	
	int i;
	char c;

	i = 0;
	while (i <= 9)
	{
		c = '0' + i;		// ConversÃ£o de Int para Char
		write(1, &c, 1);
		i++;
	}
	*/
}
	